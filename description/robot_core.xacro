<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro"  >

<xacro:include filename="inertial_macros.xacro"/>

    <material name="white">
        <color rgba="1 1 1 1"/>
    </material>
    <material name="black">
        <color rgba="0 0 0 1"/>
    </material>
    <material name="orange">
        <color rgba="1 0.3 0.1 1"/>
    </material>
    <material name="yellow">
        <color rgba="1 1 0 1"/>
    </material>


<!-- Base_link -->
    <link name="base_link">
        
    </link>

<!-- BASE_FOOTPRINT LINK -->

<joint name="base_footprint_joint" type="fixed">
    <parent link="base_link"/>
    <child link="base_footprint"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
</joint>

<link name="base_footprint">
</link>





<!-- Chassis_link -->
    <joint name="chassis_joint" type="fixed">
        <parent link="base_link"/>
        <child link="chassis"/>
        <origin xyz="-0.2 0.0 0.0" />
    </joint>

    <link name="chassis">
        <visual>
            <origin xyz="0.25 0 0.1" />
            <geometry>
                <box size="0.5 0.5 0.2"/>
            </geometry>
            <material name="white"/>
        </visual>
        <collision>
            <origin xyz="0.25 0 0.1" />
            <geometry>
                <box size="0.5 0.5 0.2"/>
            </geometry>
        </collision>
        <xacro:inertial_box mass="1.0" x="0.5" y="0.5" z="0.2">
            <origin xyz="0.25 0.0 0.1" rpy="0.0 0.0 0.0"/>
            
        </xacro:inertial_box>
    </link>


    <gazebo reference="chassis">
        <material>Gazebo/White</material>
    </gazebo>

<!-- Left_wheel_link -->
    <joint name="left_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="left_wheel"/>
        <origin xyz="0.0 0.25 0.0" rpy="-${pi/2} 0.0 0.0"/>
        <axis xyz="0.0 0.0 1.0"/>
        
    </joint>

    <link name="left_wheel">
        <visual>
            <geometry>
                <cylinder radius="0.1" length="0.05"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.1" length="0.05"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="0.1" radius="0.1" length="0.05">
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            
        </xacro:inertial_cylinder>
    </link>

    <gazebo reference="left_wheel">
        <material>Gazebo/Black</material>
    </gazebo>

<!-- Right_wheel_link -->
    <joint name="right_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="right_wheel"/>
        <origin xyz="0.0 -0.25 0.0" rpy="${pi/2} 0.0 0.0"/>
        <axis xyz="0.0 0.0 1.0"/>
    
    </joint>

    <link name="right_wheel">
        <visual>
            <geometry>
                <cylinder radius="0.1" length="0.05"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.1" length="0.05"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="0.1" radius="0.1" length="0.05">
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            
        </xacro:inertial_cylinder>
    </link>


    <gazebo reference="right_wheel">
        <material>Gazebo/Black</material>
    </gazebo>

<!-- Caster_wheel_link -->
    <joint name="caster_wheel_joint" type="fixed">
        <parent link="chassis"/>
        <child link="caster_wheel"/>
        <origin xyz="0.4 0.0 0.0" rpy="0.0 0.0 0.0"/>       
    </joint>

    <link name="caster_wheel">
        <visual>
            <geometry>
                <sphere radius="0.1"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <sphere radius="0.1"/>
            </geometry>
        </collision>
        <xacro:inertial_sphere mass="0.1" radius="0.1">
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            
        </xacro:inertial_sphere>
        
    </link>
       
        
    <gazebo reference="caster_wheel">
        <material>Gazebo/Black</material>
        <mu1 value="0.001"/>
        <mu2 value="0.001"/>
    </gazebo>   
    
    
<!-- Left_Pole_Link -->
<joint name="left_pole_joint" type="fixed">
    <parent link="chassis"/>
    <child link="left_pole"/>
    <origin xyz="0.3 0.2 0.7" rpy="0.0 0.0 0.0"/>
    <!-- <axis xyz="0.0 0.0 1.0"/>  -->
    
</joint>

<link name="left_pole">
    <visual>
        <!-- <origin xyz="0.1 -0.2 0.7"/> -->
        <geometry>
            <cylinder radius="0.025" length="1.0"/>
        </geometry>
        <material name="white"/>
    </visual>
    <collision>
        <geometry>
            <cylinder radius="0.025" length="1.0"/>
        </geometry>
    </collision>
    <xacro:inertial_cylinder mass="0.2" radius="0.025" length="1.0">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        
    </xacro:inertial_cylinder>
</link>

<gazebo reference="left_pole">
    <material>Gazebo/White</material>
</gazebo> 

<!-- Right_Pole_Link -->
<joint name="right_pole_joint" type="fixed">
    <parent link="chassis"/>
    <child link="right_pole"/>
    <origin xyz="0.3 -0.2 0.7" rpy="0.0 0.0 0.0"/>
    <!-- <axis xyz="0.0 0.0 1.0"/>  -->
    
</joint>

<link name="right_pole">
    <visual>
        <!-- <origin xyz="0.1 0.2 0.7"/> -->
        <geometry>
            <cylinder radius="0.025" length="1.0"/>
        </geometry>
        <material name="white"/>
    </visual>
    <collision>
        <geometry>
            <cylinder radius="0.025" length="1.0"/>
        </geometry>
    </collision>
    <xacro:inertial_cylinder mass="0.2" radius="0.025" length="1.0">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        
    </xacro:inertial_cylinder>
</link>

<gazebo reference="right_pole">
    <material>Gazebo/White</material>
</gazebo> 
    

<!-- Panel_Link -->

<joint name="panel_joint" type="fixed">
    <parent link="chassis"/>
    <child link="panel"/>
    <origin xyz="0.15 0.0 1.1" />
</joint>

<link name="panel">
    <visual>
        <origin xyz="0.15 0.0 0.0" rpy="0.0 0.0 ${pi/2}"/>
        
        <geometry>
            <box size="0.35 0.015 0.2"/>
        </geometry>
        <material name="black"/>
    </visual>
    <collision>
        <origin xyz="0.15 0 0.0" />
        <geometry>
            <box size="0.35 0.15 0.2"/>
        </geometry>
    </collision>
    <xacro:inertial_box mass="0.2" x="0.35" y="0.15" z="0.2">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        
    </xacro:inertial_box>
</link>


<gazebo reference="panel">
    <material>Gazebo/Black</material>
</gazebo>

<!-- Tray_Link -->
<joint name="tray_joint" type="fixed">
    <parent link="chassis"/>
    <child link="tray_link"/>
    <origin xyz="0.15 0.0 0.7" />
</joint>

<link name="tray_link">
    <visual>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        
        <geometry>
            <box size="0.5 0.5 0.05"/>
        </geometry>
        <material name="white"/>
    </visual>
    <collision>
        <origin xyz="0.0 0 0.0" />
        <geometry>
            <box size="0.5 0.5 0.05"/>
        </geometry>
    </collision>
    <xacro:inertial_box mass="0.2" x="0.5" y="0.5" z="0.05">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        
    </xacro:inertial_box>
</link>


<gazebo reference="tray_link">
    <material>Gazebo/White</material>
</gazebo>


</robot>